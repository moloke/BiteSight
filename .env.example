# Environment variables for BiteSight

# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
EXPO_PUBLIC_FIREBASE_APP_ID=your_app_id

# API Base URL (Cloud Functions)
EXPO_PUBLIC_API_BASE_URL=https://us-central1-your_project.cloudfunctions.net

# Sentry (Error Tracking)
SENTRY_DSN=your_sentry_dsn_here

# Google Cloud Vision API (for OCR)
GOOGLE_CLOUD_VISION_API_KEY=your_google_cloud_vision_api_key_here

# Firebase Cloud Functions
FIREBASE_FUNCTIONS_URL=https://us-central1-your-project-id.cloudfunctions.net/api

# Note: Backend API keys (OpenAI, Unsplash, Pexels) should be stored
# in Firebase Cloud Functions environment variables, NOT here
